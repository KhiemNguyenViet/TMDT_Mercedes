
RewriteEngine on
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ https://%1/$1 [R=301,L]
RewriteRule ^index.html index.php
RewriteRule ^huongdansudung.html huongdansudung.php
RewriteRule ^baoduongdinhky.html baoduongdinhky.php
RewriteRule ^dichvucuuhoc.html dichvucuuhoc.php
RewriteRule ^tuvanmienphi.html tuvanmienphi.php
RewriteRule ^giaoxe.html giaoxe.php
RewriteRule ^doitra.html doitra.php
RewriteRule ^login.html login.php
RewriteRule ^register.html register.php
RewriteRule ^thuonghieu.html thuonghieu.php
RewriteRule ^cacmauxe.html cacmauxe.php
RewriteRule ^lich_laithu.html lich_laithu.php
RewriteRule ^lichsu_thanhtoan.html lichsu_thanhtoan.php
RewriteRule ^account.html account.php
RewriteRule ^dat-cho-xe.html datcho.php
RewriteRule ^logout process_logout.php
RewriteRule ^admincp/edit-product$ admin/index.php?action=edit_product [QSA,L]
RewriteRule ^admincp/edit-user/([0-9]+)$ admin/index.php?action=edit_user&id=$1 [QSA,L]
RewriteRule ^admincp/edit-product/([0-9]+)$ admin/index.php?action=edit_product&id=$1 [QSA,L]
RewriteRule ^admin/dashboard admin/index.php?action=dashboard
RewriteRule ^product/([^/]+).html view.php?blank=$1
RewriteRule ^bai-viet/([^/]+).html tintuc.php?blank=$1
RewriteRule ^tonghopthongtin.html tonghopthongtin.php
